<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	      http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	      version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<description></description>
  	<display-name>CreateCustomer</display-name>
  	<servlet-name>CreateCustomer</servlet-name>
  	<servlet-class>com.application.container.CreateCustomer</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>Banks</display-name>
  	<servlet-name>Banks</servlet-name>
  	<servlet-class>com.application.container.AllBanks</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>AccountAuthorization</display-name>
  	<servlet-name>AccountAuthorization</servlet-name>
  	<servlet-class>com.application.container.AccountAuthorization</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>Logout</display-name>
  	<servlet-name>Logout</servlet-name>
  	<servlet-class>com.application.container.Logout</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>Withdraw</display-name>
  	<servlet-name>Withdraw</servlet-name>
  	<servlet-class>com.application.container.Withdraw</servlet-class>
  </servlet>
  <servlet>
  	<description></description>
  	<display-name>Deposit</display-name>
  	<servlet-name>Deposit</servlet-name>
  	<servlet-class>com.application.container.Deposit</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CreateCustomer</servlet-name>
  	<url-pattern>/allBanks</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Banks</servlet-name>
  	<url-pattern>/bankMenu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AccountAuthorization</servlet-name>
  	<url-pattern>/account</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Withdraw</servlet-name>
  	<url-pattern>/withdraw</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Deposit</servlet-name>
  	<url-pattern>/deposit</url-pattern>
  </servlet-mapping>
</web-app>
